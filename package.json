{
  "name": "ever-lab-os",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "emulators": "firebase emulators:start",
    "dev:all": "concurrently \"npm run dev\" \"npm run emulators\" --names \"APP,EMU\" --prefix-colors \"cyan,yellow\"",
    "analyze": "npm run analyze:circular && npm run analyze:duplicates && npm run analyze:unused",
    "analyze:circular": "madge --circular --extensions ts,tsx src",
    "analyze:duplicates": "jscpd src",
    "analyze:unused": "knip"
  },
  "dependencies": {
    "@google/genai": "^1.25.0",
    "@testing-library/react": "^16.3.0",
    "firebase": "^10.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "firebase-tools": "^13.0.0",
    "jscpd": "^4.0.5",
    "jsdom": "^27.0.0",
    "knip": "^5.66.0",
    "madge": "^8.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^1.0.0"
  }
}
